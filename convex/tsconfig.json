{
    "compilerOptions":{
        "allowJs": true,
        "strict": true,
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,

        "target": "ESNext",
        "lib": ["ES2021", "dom"],
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "noEmit": true,
    },
    "include": ["./**/*"],
    "exclude": ["./_generated"]
}